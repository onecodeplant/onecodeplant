# OneCodePlant Configuration Example
# Copy this file to .env and set your actual values

# AI Engine Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# AI Engine Selection (openai, anthropic, google)
ONECODE_AI_ENGINE=openai

# AI Model Overrides (optional)
ONECODE_AI_MODEL=gpt-4o
# ONECODE_AI_MODEL=claude-3-5-sonnet-20241022
# ONECODE_AI_MODEL=gemini-pro

# AI Parameters
ONECODE_AI_TEMPERATURE=0.1
ONECODE_AI_MAX_TOKENS=1000

# CLI Behavior
ONECODE_DRY_RUN=false
ONECODE_AUTO_EXECUTE=false
ONECODE_SAFETY_CHECKS=true

# Logging Configuration
ONECODE_LOG_LEVEL=INFO
ONECODE_LOG_DIR=~/.onecode/logs

# ROS 2 Environment (if applicable)
ROS_DISTRO=humble
ROS_VERSION=2
AMENT_PREFIX_PATH=/opt/ros/humble